{
  "name": "ch5-utilities-webpack-plugin",
  "version": "0.0.5",
  "description": "CH5 Utilities Webpack plugin",
  "private": false,
  "main": "build/index.js",
  "types": "build/@types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "build:prod": "yarn clean && yarn tsc",
    "clean": "rimraf build",
    "publish:local": "yarn build && yarn unlink && yarn link",
    "setup:local": "yarn install && yarn link @crestron/ch5-utilities && yarn build && yarn link",
    "tsc": "tsc --declaration --declarationDir build/@types",
    "start": "ts-node ./src/index.ts"
  },
  "author": "Crestron",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "@types/lodash": "^4.14.129",
    "@types/node": "^11.12.0",
    "@types/webpack": "^4.4.31",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.11",
    "ts-node": "^8.0.3",
    "tsc": "^1.20150623.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.0"
  }
}
